<%= form_with model: model, method: method, url: url, class: 'merchant_item_form', local: true do |f| %>
  <%= f.label :percent_discount %>
  <%= f.number_field :percent_discount, step: :any %><br/><br/>
  <%= f.label :quantity_threshold %>
  <%= f.number_field :quantity_threshold %><br/><br/>
  <%= f.submit 'Submit'%>
<% end %>
