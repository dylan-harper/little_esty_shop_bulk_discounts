<%= form_with model: model, method: method, url: url, class: 'merchant_bulk_discount_form', local: true do |f| %>
  <%= f.label :percent_discount %>
  <%= f.number_field :percent_discount, step: :any, class: 'form-control',id:"percent_discount" %><br/><br/>
  <%= f.label :quantity_threshold %>
  <%= f.number_field :quantity_threshold, class: 'form-control',id:"quantity_threshold" %><br/><br/>
  <%= f.submit 'Submit'%>
<% end %>
